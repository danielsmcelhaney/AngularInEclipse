/**
 * 
 */
(function(ca){
	'use strict';
	ca.angular = angular.module("myApp", ["ngRoute"]);
	ca.angular.config(function($routeProvider) {
	    $routeProvider
	    .when("/", {
	    	controller: 'homeController',
	        templateUrl : 'home.html'	
	    });
	});
})(window.myApp || (window.myApp = {}));